table user {
id int [pk]
name varchar
email varchar
password varchar

} 

table hikes {
id int [pk]
name varchar
location varchar
length int 
details text 
userId int [ref: > user.id]
difficultyId int [ref: > difficulty.id]
}

table difficulty {
id int [pk]
name varchar 

}

